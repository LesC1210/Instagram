target 'instaflo' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for instaflo
  pod 'Parse'
  pod 'AFNetworking', '~> 3.0'

  target 'instafloTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'instafloUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
